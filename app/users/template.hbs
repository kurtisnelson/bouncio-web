<h2>Users</h2>
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Email</th>
      <th>App</th>
    </tr>
  </thead>
  <tbody>
  {{#each model as |user|}}
    <tr>
      <td>{{#link-to "user" user}}{{user.id}}{{/link-to}}</td>
        <td>{{user.maskedEmail}}</td>
      <td>{{user.app.name}}</td>
    </tr>
  {{/each}}
</tbody>
</table>
{{outlet}}
